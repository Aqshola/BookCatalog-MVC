<tr>
  <th><%- indeks%></th>
  <td><%- book.title%></td>
  <td><%- book.type %></td>
  <td>
    <%- new Intl.NumberFormat("EN", { style: "currency", currency: "IDR", })
    .format(book.price) .replace("IDR", "Rp") .replace(".00", ""); %>
  </td>
  <td>
    <a class="btn btn-primary" href="/admin/edit-book/<%- book._id %>">
      Edit
    </a>
  </td>
  <td></td>
  <td>
    <form method="POST" action="/admin/remove-book/<%- book._id %> ">
      <button type="submit" class="btn btn-danger delbtn">Delete</button>
    </form>
  </td>
</tr>
